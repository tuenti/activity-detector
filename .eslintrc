env:
  es6: true
  browser: true
  node: true
parserOptions:
  ecmaVersion: 6
  sourceType: "module"
rules:
  arrow-spacing: 2
  brace-style: [2, 1tbs, {allowSingleLine: true}]
  comma-spacing: 2
  comma-style: [2, last]
  consistent-return: 2
  constructor-super: 2
  curly: [2, multi-line]
  default-case: 2
  dot-notation: 2
  eqeqeq: 2
  generator-star-spacing: [2, after]
  indent: [2, 4, {SwitchCase: 1}]
  jsx-quotes: [1, prefer-double]
  linebreak-style: [2, unix]
  key-spacing: [2, {beforeColon: false, afterColon: true}]
  new-parens: 2
  new-cap: 2
  no-array-constructor: 2
  no-caller: 2
  no-class-assign: 2
  no-cond-assign: 2
  no-const-assign: 2
  no-console: 2
  no-constant-condition: 1
  no-continue: 2
  no-debugger: 2
  no-delete-var: 2
  no-dupe-args: 2
  no-dupe-keys: 2
  no-duplicate-case: 2
  no-empty: 2
  no-eval: 2
  no-extend-native: 2
  no-extra-bind: 2
  no-extra-boolean-cast: 2
  no-extra-semi: 2
  no-fallthrough: 2
  no-floating-decimal: 2
  no-implied-eval: 2
  no-inner-declarations: [2, both]
  no-invalid-regexp: 2
  no-irregular-whitespace: 2
  no-iterator: 2
  no-labels: 2
  no-lone-blocks: 2
  no-lonely-if: 2
  no-loop-func: 2
  no-mixed-spaces-and-tabs: 2
  no-multi-spaces: 2
  no-native-reassign: 2
  no-negated-in-lhs: 2
  no-new: 2
  no-new-func: 2
  no-new-object: 2
  no-new-wrappers: 2
  no-obj-calls: 2
  no-octal: 2
  no-proto: 2
  no-redeclare: 2
  no-return-assign: 2
  no-shadow-restricted-names: 2
  no-spaced-func: 2
  no-sparse-arrays: 2
  no-this-before-super: 2
  no-trailing-spaces: 2
  no-undef: 2
  no-undef-init: 2
  no-underscore-dangle: 2
  no-unreachable: 2
  no-unused-expressions: 2
  no-unused-vars: [2, {vars: all, args: after-used}]
  no-use-before-define: 2
  no-var: 2
  no-with: 2
  operator-linebreak: [2, before]
  prefer-spread: 1
  prefer-arrow-callback: 1
  prefer-template: 1
  quotes: [2, single, avoid-escape]
  semi: [2, always]
  semi-spacing: 2
  keyword-spacing: 2
  space-before-function-paren: [2, {anonymous: always, named: never}]
  space-in-parens: [2, never]
  space-infix-ops: [2, {int32Hint: true}]
  space-unary-ops: 2
  strict: [2, global]
  use-isnan: 2
  valid-typeof: 2
  wrap-iife: [2, inside]
  wrap-regex: 2